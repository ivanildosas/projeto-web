	<repositories>
		<repository>
			<id>prime</id>
			<name>prime</name>
			<url>http://repository.primefaces.org/</url>
			<layout>default</layout>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
	</repositories>

	<pluginRepositories>
		<pluginRepository>
			<id>prime</id>
